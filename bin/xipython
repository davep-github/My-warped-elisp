#!/bin/bash

source script-x
set -u
progname="$(basename $0)"
source eexec
eexec_program=$(EExec_parse "$@")
for op in $eexec_program
do
  $op
  shift
done
unset eexec_program
one=${1-}
###: ${ipython_bin=${DP_DEF_IPYTHON_BIN-${one-ipython}}}

: ${ipython_bin=${DP_DEF_IPYTHON_BIN-${1-ipython}}}

case "${one}" in
    [0-9]*) ipython_bin="ipython${one}";;
esac

# @todo XXX Write an analogous ipython(dp) command and use that.
EExec dpxx "${ipython_bin}"

