def main(argv):
    import getopt
    opt_string = ""
    opts, args = getopt.getopt(argv[1:], opt_string)
    for o, v in opts:
        if o == '-<option-letter>':
            # Handle opt
            continue

if __name__ == "__main__":
    main(sys.argv)


