#!/bin/bash

source script-x
set -u
progname="$(basename $0)"
source eexec
if vsetp "${eexec_program-}"    # Did the caller provide a program?
then
    EEXEC_SHIFT=:
else
    eexec_program=$(EExec_parse "$@")
    EEXEC_SHIFT=shift
fi

for op in $eexec_program
do
  $op
  ${EEXEC_SHIFT}
done
EExec_verbose_msg $(echo_id eexec_program)
unset eexec_program
# Or export eexec_program to propagate eexec info to a called program.
# export eexec_program

#export BASE
#export LOCAL
#export REMOTE
#export MERGED

#EExec_verbose_msg "$>$@<"
#EExec_verbose_echo_id BASE
#EExec_verbose_echo_id LOCAL
#EExec_verbose_echo_id REMOTE
#EExec_verbose_echo_id MERGED


ec-diff --merge "$@"

