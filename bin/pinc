#!/usr/bin/env python

import sys, os

def main(argv):
    import getopt
    opt_string = ""
    opts, args = getopt.getopt(argv[1:], opt_string)
    for o, v in opts:
        if o == '-<option-letter>':
            # Handle opt
            continue
    for arg in args:
        # Handle arg
        pass

    a = float(eval(argv[1]))
    b = float(eval(argv[2]))

    pinc = (b - a) / a
    print pinc * 100

if __name__ == "__main__":
    main(sys.argv)


