#!/bin/sh

source script-x
set -u
progname="$(basename $0)"
source eexec
eexec_program=$(EExec_parse "$@")
for op in $eexec_program
do
  $op
  shift
done
unset eexec_program

[ "$#" = 2 ] || {
    echo "$progname: Usage: old-name new-name"
    exit 1
} 1>&2

old_file="${1}"; shift
new_file="${1}"; shift

p4 integrate "${old_file}" "${new_file}"
