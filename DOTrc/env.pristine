# Set up enough of an environment to be useful.
PATH="$HOME/bin.simp:$PATH"
export YOCTO_DIR=/local/work/vet/yocto

alias ls='ls -F'
ltl()
{
    ls -lt "$@" | less
}

lth()
{
    ls -lt "$@" | head -n 40
}

alias h='history | head -n 40'
alias hless='history | less'

