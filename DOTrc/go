# ${HOME} is special in that it does not end with a /
Ee|davep|               ${HOME}
Ee|elm|			${davep}/.elm/
Ee|jobs|		${davep}/jobs/
Ee|ssh|			${davep}/.ssh/
Ee|pine|		${davep}/pine/
Ee|ppp|			${davep}/.ppp/
Ee|ulb|			/usr/local/bin/
Ee|yokel|               ${davep}/yokel/
Ee|yokelbin|yb|         ${yokel}bin/
Ee|yokelsbin|ysb|       ${yokel}sbin/
Ee|yokeletc|ye|         ${yokel}etc/
Ee|yokellib|yl|         ${yokel}lib/
q|noway|		no-way-we-see-this-since-selector-is-q
Ee|pdb|			${davep}/etc/pdb/
Ee|pydb|		${davep}/etc/pydb/
Ee|doc|			${davep}/doc/
Ee|inb|                 ${davep}/inb/
E|rc|                   ${davep}/.rc/
Ee|bin|			${davep}/bin/
Ee|lisp|		${davep}/lisp/
Ee|dpbuild|dpbld|       ${davep}/davep/build/
Ee|usdoc|usd|           /usr/share/doc/

#E|elisponly|           a test which should not match because of comment
Ee|testA|testB|testC| 	value-for-testABC

# Other things can be expanded... sort of. Needs work.  These make the most
# sense in emacs shells, plus the abbrev file format can handle trailing
# spaces.
#Ee|faf|                 find . -type f -print0 | xargs -r0 
#Ee|faff|                find . -type f -print0 | xargs -r0 fgrep 
#Ee|fafe|                find . -type f -print0 | xargs -r0 egrep

# dpw git dir (dpw/dpw) is visited *FAR* more often than dpw (dpw), so remove
# dpwg and just use dpw for what was dpwg.
Ee|dpwgit|dpwg|              ${davep}/dpw/dpw/
Ee|dpworld|dpw|              ${dpwgit}
Ee|dpwbuild|dpwbld|dpwbld|   ${dpworld}build/
Ee|dpylib|                   ${davep}/lib/pylib/
Ee|lubuntuconf|lubconf|      ${davep}/.config/lxsession/Lubuntu/
Ee|obconf|                   ${davep}/.config/openbox/
Ee|obrc|                     ${obconf}/rc.xml
Ee|obmenu|                   ${obconf}/menu.xml
Ee|cscopedbroot|             000-cscope-db-root
Ee|csdb|                     ${cscopedbroot}
Ee|ipystartup|ipyup|ipyrc|   ${davep}/.config/ipython/profile_default/startup/

