startup_message         off
escape                  ^x^x
autonuke                off
#term                    vt100
#term                    linux
setenv                  IMASCREENINSTANCE 1
vbell                   on
ignorecase              on
defscrollback           ${SCREEN_DEFSCROLLBACK}
markkeys                "0=^A:$=^E"
bufferfile              ${SCREENDATA_EXCHANGE}
hardcopydir             ${SCREENDATA_HARDCOPY_DIR}
bind ^/ help
bind ^h help
bind ^e quit
bind ^b copy
bind / screen
