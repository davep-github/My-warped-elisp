startup_message         off
escape                  ^x^x
autonuke                off
#term                    vt100
#term                    linux
setenv                  IMASCREENINSTANCE 1
vbell                   on
ignorecase              on
defscrollback           4000
markkeys                "0=^A:$=^E"
bufferfile              ${HOME}/.screen/generic.d/data.d/screen-exchange
hardcopydir             ${HOME}/.screen/generic.d/data.d/hardcopy.d
bind ^/ help
bind ^h help
bind ^e quit
bind ^b copy
bind / screen
